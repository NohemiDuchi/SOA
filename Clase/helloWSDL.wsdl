<?xml version="1.0" encoding="UTF-8"?>
<definitions name="helloWSDL" targetNamespace="http://j2ee.netbeans.org/wsdl/Hello/java/helloWSDL"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://j2ee.netbeans.org/wsdl/Hello/java/helloWSDL" xmlns:ns="http://xml.netbeans.org/schema/hello" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
    <types>
        <xsd:schema targetNamespace="http://j2ee.netbeans.org/wsdl/Hello/java/helloWSDL">
            <xsd:import namespace="http://xml.netbeans.org/schema/hello" schemaLocation="hello.xsd"/>
        </xsd:schema>
    </types>
    <message name="sayHelloRequest">
        <part name="part1" element="ns:sayHello"/>
    </message>
    <message name="sayHelloResponse">
        <part name="parameters" element="ns:sayHelloResponse"/>
    </message>
    <portType name="Hello">
        <operation name="sayHello">
            <input name="input1" message="tns:sayHelloRequest"/>
            <output name="output1" message="tns:sayHelloResponse"/>
        </operation>
    </portType>
    <binding name="helloPortBinding" type="tns:Hello">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="sayHello">
            <soap:operation/>
            <input name="input1">
                <soap:body use="literal"/>
            </input>
            <output name="output1">
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>
    <service name="helloService">
        <port name="helloPort" binding="tns:helloPortBinding">
            <soap:address location="http://localhost:${HttpDefaultPort}/helloService/helloPort"/>
        </port>
    </service>
</definitions>
